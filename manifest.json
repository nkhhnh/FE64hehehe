{
    "name": "Key In Cloud Music",
    "short_name": "KICM",
    "description": "A progressive web app for playing music online and offline",
    "start_url": "/html/index.html",
    "scope": "/html/",
    "display": "fullscreen",
    "background_color": "#ffffff",
    "theme_color": "#e5d1ad",

    "icons": [
      {
        "src": "/image/192x192.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "/image/512x512.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ],
    "background": {
      "service_worker": "sw.js",
      "persistent": true
    },
    "permissions": [
      "background",
      "audioPlayback"
    ]
  }